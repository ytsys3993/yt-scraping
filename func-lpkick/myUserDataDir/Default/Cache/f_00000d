<script>
    Common.init = function() {
            }

    function normallogin() {
        if (navigator.cookieEnabled === false) {
            alert("端末のCookieの設定が無効です。\nCookieを有効にしてください。");
            return false;
        }

        showProgress('認証中です..');
        $("input[name='login']").blur();
        $("input[name='password']").blur();
        API.post(
            'sessions/create', {
                'login': $("input[name='login']").prop('value'),
                'password': $("input[name='password']").prop('value'),
                'auto_login': $("input:checked").prop('value'),
                'login_password': $("input[name='login']").prop('value') + "&" + $("input[name='password']").prop('value')
            },
            login_callback
        );
    }

    function login_callback(data) {
        var result = data['success'];
        if (result) {
            // TODO: 元来のアクセス先URLへリダイレクトする.
            //document.location.href = 'https://t.livepocket.jp/my_top/';
            document.location.href = 'https://t.livepocket.jp/e/wakkuru202110';
        } else {
            hideProgress();
        }
    }

    function snslogin(sns_type) {
        if (navigator.cookieEnabled === false) {
            alert("端末のCookieの設定が無効です。\nCookieを有効にしてください。");
            return false;
        }

        showProgress('認証中です..');
        $('#form').attr('action', 'https://t.livepocket.jp/api/' + sns_type + '/authorize');
        $('#form').attr('onsubmit', 'return true');
        $('#form > #hidden_type').attr('value', '2');
        $('#form > #hidden_redirect_url').attr('value', 'https://t.livepocket.jp/e/wakkuru202110');
        $('#form').submit();
    }

    function new_regist() {
        document.location.href = 'https://t.livepocket.jp/registration/';
        return;
    }

    // 入力するパスワードを表示する
    function check_show_password(e) {
        var pass = document.getElementById("password");
        if (e.checked) {
            pass.setAttribute("type", "text");
        } else {
            pass.setAttribute("type", "password");
        }
    }

    // plusidイベントハンドラ
    gigya.accounts.addEventHandlers({
        onLogin: onLoginHandler
    });

    // jQuery Mobile での ajax ローディングが +ID 関連処理と競合するので無効にする
    $(document).ready(function() {
        $.mobile.ajaxEnabled = false;
    });

    /**
     * screenSet の表示
     * @param string type onLoginHandler で使用する処理区分 login:ログイン処理 / regist:登録処理
     */
    function showScreenSet(type) {
        if (navigator.cookieEnabled === false) {
            alert("端末のCookieの設定が無効です。\nCookieを有効にしてください。");
            return false;
        }

        // onLoginHandler で使用する処理区分を cookie に設定する
        // 有効時間は1時間
        delete_cookie("plusid-login-event", "/");
        set_cookie("plusid-login-event", type, 3600, "/");

        var plusid_linkage_invalidation_flg = $('#plusid_linkage_invalidation_flg').val();
        if (plusid_linkage_invalidation_flg == 0) {
            var lang = {
                name: "日本語",
                code: "ja",
                en: "Japanese"
            };
            try {
                var wovn = WOVN.io.getCurrentLang(); // Object {name: "English", code: "en", en: "English"}
                if (wovn) {
                    lang = wovn;
                }
            } catch (e) {}

            gigya.accounts.showScreenSet({
                screenSet: 'plusid-RegistrationLogin',
                startScreen: 'gigya-login-screen',
                deviceType: 'mobile',
                lang: lang.code,
                authFlow: 'redirect',
            });
        } else {
            alert('現在、+ID連携はご利用になれません');
        }
    }

    /**
     * plusidログインイベント
     */
    function onLoginHandler(obj) {
        if ((obj.loginMode === 'standard' || parseInt(obj.errorCode) === 0) && obj.UID && obj.profile.email) {
            // cookie に設定されているはずの処理区分を取得する
            // 取得後は削除する
            const type = get_cookie()["plusid-login-event"];
            delete_cookie("plusid-login-event", "/");

            if (type === 'regist') {
                // 新規登録（+IDをお持ちの方）
                regist_with_plusid(obj.UID, obj.profile.email);
            } else {
                // SNSログイン（+IDログイン）
                showProgress('認証中です..');
                try {
                    API.post(
                        'plusid/authorize', {
                            'UID': obj.UID
                        },
                        function(data) {
                            if (data && data['success']) {
                                location.href = 'https://t.livepocket.jp/e/wakkuru202110';
                            } else {
                                hideProgress();
                            }
                        }
                    );
                } catch (e) {
                    hideProgress();
                    showNavigation('+IDログインできませんでした');
                }
            }
        } else {
            //showNavigation('+IDログインできませんでした');
        }
    }

    // plusid ScreenSet画面切替イベント （AVEX-1393で使わなくなった）
    // function onAfterScreenLoadHandler(obj) {
    //     // スクロール対応
    //     document.querySelector('.gigya-screen-dialog-main').scrollTop = 0;
    // }

    function regist_with_plusid(uid, email) {
        $('#plusid_uid').val(uid);
        $('#plusid_email').val(email);
        $('#form_regist_with_plusid').submit();
    }

    </script>
