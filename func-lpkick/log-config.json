{
    "appenders": {
      "console": {
        "type": "console"
      },
      "system": {
        "type": "dateFile",
        "filename": "log/system.log",
        "pattern": "-yyyy-MM-dd",
        "keepFileExt": true
      }
    },
    "categories": {
      "default": {
        "appenders": [
          "console",
          "system"
        ],
        "level": "all"
      }
    }
  }